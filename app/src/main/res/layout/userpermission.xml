<?xml version="1.0" encoding="utf-8"?>

<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:fillViewport="true">
    <android.support.v4.widget.SwipeRefreshLayout
        android:id="@+id/swipeRefreshLayout"
        android:layout_width="match_parent"
        android:layout_height="match_parent">


        <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="10dp">


        <LinearLayout
            android:id="@+id/advanceLayer"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:padding="5dp"
            android:orientation="horizontal"
            android:gravity="center">
            <TextView
                android:id="@+id/advanceUser"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:text="Advance"
                android:layout_margin="8dp"
                android:textSize="15sp"/>
            <Button
                android:id="@+id/advtoggBtn"
                android:layout_width="match_parent"
                android:layout_height="30dp"
                android:text="Give"
                android:textAllCaps="false"
                android:layout_weight="1"
                android:gravity="center"
                android:background="@drawable/add_cancel_button_shape"/>
        </LinearLayout>
        <LinearLayout
            android:id="@+id/PlaceOrderLayer"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:padding="5dp"
            android:gravity="center"

            android:orientation="horizontal"
            android:layout_below="@+id/advanceLayer">
            <TextView
                android:id="@+id/placeOrderUser"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:text="PlaceOrder"
                android:layout_margin="8dp"
                android:textSize="15sp"/>
            <Button
                android:id="@+id/PlaceOrderLayertoggBtn"
                android:layout_width="match_parent"
                android:layout_height="30dp"
                android:layout_weight="1"
                android:gravity="center"
                android:textAllCaps="false"
                android:text="Give"
                android:background="@drawable/add_cancel_button_shape"/>


        </LinearLayout>
        <LinearLayout
            android:id="@+id/HoldLayer"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:padding="5dp"
            android:orientation="horizontal"

            android:gravity="center"
            android:layout_below="@+id/PlaceOrderLayer">
            <TextView
                android:id="@+id/holdaUser"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:layout_margin="8dp"
                android:text="Hold"
                android:textSize="15sp"/>

            <Button
                android:id="@+id/HoldtoggBtn"
                android:layout_width="match_parent"
                android:layout_height="30dp"
                android:layout_weight="1"
                android:text="Give"
                android:textAllCaps="false"
                android:gravity="center"
                android:background="@drawable/add_cancel_button_shape"/>

        </LinearLayout>
        <LinearLayout
            android:id="@+id/MaterialReceiveLayer"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:padding="5dp"
            android:gravity="center"

            android:orientation="horizontal"
            android:layout_below="@+id/HoldLayer">
            <TextView
                android:id="@+id/materialUser"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:text="MaterialReceive"
                android:layout_margin="8dp"
                android:textSize="15sp"/>
            <Button
                android:id="@+id/MaterialReceiveLayertoggBtn"
                android:layout_width="match_parent"
                android:layout_height="30dp"
                android:layout_weight="1"
                android:text="Give"
                android:textAllCaps="false"
                android:gravity="center"
                android:background="@drawable/add_cancel_button_shape"/>

        </LinearLayout>
        <LinearLayout
            android:id="@+id/HandOverLayer"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:gravity="center"
            android:padding="5dp"

            android:orientation="horizontal"
            android:layout_below="@+id/MaterialReceiveLayer"
           >
            <TextView
                android:id="@+id/handoverUser"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:layout_margin="8dp"
                android:text="HandOver"
                android:textSize="15sp"/>

            <Button
                android:id="@+id/HandOverLayertoggBtn"
                android:layout_width="match_parent"
                android:layout_height="30dp"
                android:layout_weight="1"
                android:text="Give"
                android:textAllCaps="false"
                android:gravity="center"
                android:background="@drawable/add_cancel_button_shape"/>
        </LinearLayout>
        <LinearLayout
            android:id="@+id/ReceiveLayer"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:layout_margin="5dp"
            android:orientation="horizontal"

            android:gravity="center"
            android:layout_below="@+id/HandOverLayer">
            <TextView
                android:id="@+id/receiverUser"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:layout_margin="8dp"
                android:text="Receive"
                android:textSize="15sp"/>
            <Button
                android:id="@+id/ReceivetoggBtn"
                android:layout_width="match_parent"
                android:layout_height="30dp"
                android:layout_weight="1"
                android:text="Give"
                android:textAllCaps="false"
                android:gravity="center"
                android:background="@drawable/add_cancel_button_shape"/>

        </LinearLayout>
        <LinearLayout
            android:id="@+id/DispatchLayer"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:padding="5dp"

            android:gravity="center"
            android:orientation="horizontal"
            android:layout_below="@+id/ReceiveLayer">
            <TextView
                android:id="@+id/dispatchUser"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:text="Dispatch"
                android:layout_margin="8dp"
                android:textSize="15sp"/>
            <Button
                android:id="@+id/DispatchLayertoggBtn"
                android:layout_width="match_parent"
                android:layout_height="30dp"
                android:layout_weight="1"
                android:text="Give"
                android:textAllCaps="false"
                android:gravity="center"
                android:background="@drawable/add_cancel_button_shape"/>

        </LinearLayout>
        <LinearLayout
            android:id="@+id/OrderCreateLayer"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:padding="5dp"

            android:gravity="center"
            android:orientation="horizontal"
            android:layout_below="@+id/DispatchLayer">
            <TextView
                android:id="@+id/OrderCreateUser"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:text="OrderCreate"
                android:layout_margin="8dp"
                android:textSize="15sp"/>
            <Button
                android:id="@+id/OrderCreateLayertoggBtn"
                android:layout_width="match_parent"
                android:layout_height="30dp"
                android:layout_weight="1"
                android:text="Give"
                android:textAllCaps="false"
                android:gravity="center"
                android:background="@drawable/add_cancel_button_shape"/>

        </LinearLayout>
        <LinearLayout
            android:id="@+id/PartyLayer"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:padding="5dp"

            android:gravity="center"
            android:orientation="horizontal"
            android:layout_below="@+id/OrderCreateLayer">
            <TextView
                android:id="@+id/PartyUser"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:text="Party"
                android:layout_margin="8dp"
                android:textSize="15sp"/>
            <Button
                android:id="@+id/PartyLayertoggBtn"
                android:layout_width="match_parent"
                android:layout_height="30dp"
                android:layout_weight="1"
                android:text="Give"
                android:textAllCaps="false"
                android:gravity="center"
                android:background="@drawable/add_cancel_button_shape"/>

        </LinearLayout>
        <LinearLayout
            android:id="@+id/SearchLayer"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:padding="5dp"

            android:gravity="center"
            android:orientation="horizontal"
            android:layout_below="@+id/PartyLayer">
            <TextView
                android:id="@+id/SearchUser"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:text="Search"
                android:layout_margin="8dp"
                android:textSize="15sp"/>
            <Button
                android:id="@+id/SearchLayertoggBtn"
                android:layout_width="match_parent"
                android:layout_height="30dp"
                android:layout_weight="1"
                android:text="Give"
                android:textAllCaps="false"
                android:gravity="center"
                android:background="@drawable/add_cancel_button_shape"/>

        </LinearLayout>
        <LinearLayout
            android:id="@+id/PrintLayer"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:padding="5dp"

            android:gravity="center"
            android:orientation="horizontal"
            android:layout_below="@+id/SearchLayer">
            <TextView
                android:id="@+id/PrintUser"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:text="Print"
                android:layout_margin="8dp"
                android:textSize="15sp"/>
            <Button
                android:id="@+id/PrintLayertoggBtn"
                android:layout_width="match_parent"
                android:layout_height="30dp"
                android:text="Give"
                android:textAllCaps="false"
                android:layout_weight="1"
                android:gravity="center"
                android:background="@drawable/add_cancel_button_shape"/>

        </LinearLayout>
        </RelativeLayout>
    </android.support.v4.widget.SwipeRefreshLayout>
</ScrollView>